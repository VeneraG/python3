class Circle:
    @staticmethod
    def ravno(a, b):
        if a == b:
            print("odinak")
        else:
            print("ne odinak")

    @staticmethod
    def sravnenie(a, b):
        if a > b:
            print("a is bigger as b")
        else:
            print("b is bigger as a")

    @staticmethod
    def uvelichit(a, r):
        return a + r






